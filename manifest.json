{
  "manifest_version": 3,
  "name": "Timo's Theme",
  "version": "1.0.0",
  "description": "Happy Birthday Timo! Enjoy this personal Persona themed Theme (hehe, wordplays)",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "16": "img/logos/logo_16.png",
    "32": "img/logos/logo_32.png",
    "48": "img/logos/logo_48.png",
    "128": "img/logos/logo_128.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "img/logos/logo_16.png",
      "32": "img/logos/logo_32.png",
      "48": "img/logos/logo_48.png",
      "128": "img/logos/logo_128.png"
    },
    "default_title": "Settings",
    "default_popup": "settings.html"
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["data/*"]
    },
    {
      "matches": ["<all_urls>"],
      "resources": ["img/people/*"]
    }
  ]
}